    "preview",
    "resumable uploads",
    "xhr",
    "tus",
    "s3",
    "google drive",
    "dropbox",
    "webcam"
  ],
  "homepage": "https://uppy.io",
  "bugs": {
    "url": "https://github.com/transloadit/uppy/issues"
