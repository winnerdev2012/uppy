    "@storybook/angular": "^6.5.0-alpha.42",
    "@storybook/builder-webpack5": "^6.5.0-alpha.42",
    "@storybook/manager-webpack5": "^6.5.0-alpha.42",
    "@storybook/testing-library": "^0.0.9",
    "@types/jasmine": "~4.0.0",
    "babel-loader": "^8.2.3",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
