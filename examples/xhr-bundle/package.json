{
  "private": true,
  "name": "uppy-xhr-bundle-example",
  "scripts": {
    "css": "cp ../../packages/uppy/dist/uppy.min.css .",
    "start:client": "npm run css && budo main.js:bundle.js -- -t babelify -g aliasify",
    "start:server": "node serve.js",
    "start": "run-p start:*"
  },
  "aliasify": {
    "aliases": {
      "@uppy": "../../packages/@uppy"
    }
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "aliasify": "^2.1.0",
    "babelify": "^10.0.0",
    "budo": "^11.6.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "multer": "^1.4.1",
    "npm-run-all": "^4.1.5"
  }
}
